{
  "name": "resume",
  "version": "2.0.0",
  "engines": {
    "node": ">=12.0.0"
  },
  "description": "JSON based resume",
  "scripts": {
    "export": "npm run export:html && npm run export:pdf",
    "export:pdf": "npx resume export resume.pdf --theme onepage",
    "export:html": "npx resume export resume.html --theme onepage"
  },
  "dependencies": {
    "jsonresume-theme-onepage": "git+https://github.com/ivanph/jsonresume-theme-onepage.git",
    "resume-cli": "^1.2.7"
  }
}
